{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 300,
  "height": 400,
  "title": {
    "text": "Map of Spain",
    "subtitle": ["Equal Earth Projection"],
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "subtitlePadding": 5,
    "anchor": "middle",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/levipf/levipf.github.io/project/spain_geojson.json",
    "format": {"property": "features"}
  },
  "projection": {"type": "mercator"},
  "mark": {
    "type": "geoshape",
    "fill": "teal",
    "stroke": "white",
    "strokeWidth": 0.5
  },
  "encoding": {"tooltip": {"field": "properties.name"}}
}