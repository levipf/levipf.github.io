{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
          "title": {
        "text": "'Crypto' Search Results",
        "subtitle": "Search results are indexed to 100 = day of most search results. Source: Google Trends",
        "subtitleFontStyle": "italic",
        "subtitleFontSize": 10,
        "anchor": "start",
        "color": "black"
      },
      "width": 600,
    "height": 250,
    "description": "Google's stock price over time.",
    "data": {"url": "https://raw.githubusercontent.com/levipf/levipf.github.io/main/cryptosearch.csv"},
    "mark": {
      "type": "line"
      },
    "encoding": {
      "x": {"field": "Week", "timeUnit": "yearmonthdate", "type": "temporal", "axis": {"grid": false, "ticks": false, "labelAngle": 45}
      },
      "y": {"field": "Crypto", "title":"", "type": "quantitative"}
    }
  }
  